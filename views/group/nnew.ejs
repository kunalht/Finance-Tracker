<body class="login-page">
<div class="login-box">
  <div class="login-logo">
    <a href=""> Create new group
  </div>
  <!-- /.login-logo -->
  <div class="login-box-body">

    <form action="/newGroup" method="POST">
        <div class="form-group">
        <input name="name" type="name" class="form-control" placeholder="Group Name id="name">                
        <!-- <input name="email" type="email" class="form-control" placeholder="Email"> -->
      </div>
      <div class="form-group has-feedback">
            <input type="text" name="description" placeholder="Description" class="form-control"> <br>
            <span class="glyphicon glyphicon-lock form-control-feedback"></span>
      </div>
      <div class="row">
        <div class="col-xs-8">
          <div class="checkbox icheck">
          </div>
        </div>
        <!-- /.col -->
        <div class="col-xs-4">
          <input type="submit" class="btn btn-primary btn-block btn-flat" class="submit">Create Group  
        </div>
        <!-- /.col -->
      </div>
    </form>
 
    <!-- /.social-auth-links -->

    <!-- <a href="#">I forgot my password</a><br> -->

